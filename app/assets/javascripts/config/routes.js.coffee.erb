@Obra.config ($locationProvider, $routeProvider) ->
  $locationProvider.html5Mode(true)
  $routeProvider.when("/welcome",
    controller: "IndexController"
    template: JST["home/index"]
  ).when("/users/:id/portfolios",
    controller: "IndexController"
    template: JST["portfolios/index"]
  ).when("/comics/free",
    controller: "ComicsFreeController"
    template: JST["comics/free"]
  ).when("/comics/:id",
    controller: "ComicsShowController"
    template: JST["comics/show"]
  )